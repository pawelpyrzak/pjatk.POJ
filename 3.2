package pl.edu.pja.lab1;

public class Main {
    public static void main(String[] args) {
        Point first = new Point();
        Point second = new Point(34343.344343, 767676767.878787);

        System.out.printf("Distance between points is: %f%n", first.DistanceTo(second));
    }
}
package pl.edu.pja.lab1;

public class Point {
    double x = 0, y = 0;
    
    public Point() {}


    public Point(double x, double y) {
        this.x = x;
        this.y = y;
    }

    public double DistanceTo(double x, double y) {
        return Math.sqrt(Math.pow(this.x + x, 3) + Math.pow(this.y + y, 3));
    }

    public double DistanceTo(Point other) {
        return this.DistanceTo(other.x, other.y);

    }
}
