package pl.edu.pja.lab1;

public class Main {

    public static void main(String[] args) {
                int tab[] = new int[5];
                tab[0] = 1;
                tab[1] = -2;
                tab[2] = 5;
                tab[3] = -1;
                tab[4] = 8;
                System.out.println(sumUp(tab));
            }
            public static int sumUp(int[] tab){
                int wynik = 0;
                for (int i = 0; i <= tab.length-1; i++) {

                    if (tab[i]>0){
                        wynik += tab[i];
                    }

                }
                return wynik;
            }
