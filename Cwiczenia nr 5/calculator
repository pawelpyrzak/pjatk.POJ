package cwiczenia5;

import java.sql.SQLOutput;

public class Main {

    public static void main(String[] args) {
        Calculator calculator = new Calculator();
        System.out.println(" Wynik potegi wynosi: " + calculator.calculate(2));
        System.out.println("Wynik iloczynu wynosi: " + calculator.calculate(6, 8));
        System.out.println("Wynik sumy wynosi: " + calculator.calculate(4, 9, 1));
    }
}

package cwiczenia5;

public class Calculator {

    public int calculate(int value1){
        return value1*value1;
    }
    public int calculate(int value2, int value3){
        return value2*value3;
    }
    public int calculate(int value4, int value5, int value6){
        return value4+value5+value6;
    }
}
